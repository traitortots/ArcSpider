{
    "bindings": [
      {
        "authLevel": "function",
        "type": "httpTrigger",
        "direction": "in",
        "name": "req",
        "methods": ["get", "post"],
        "route": "HttpTrigger"
      },
      {
        "type": "http",
        "direction": "out",
        "name": "$return"
      },
      {
        "name": "outputblob",
        "type": "blob",
        "dataType": "string",
        "path": "spider-results/result.json",
        "connection": "AzureWebJobsStorage",
        "direction": "out"
      }
    ],
    "scriptFile": "../function_app.py",
    "entryPoint": "main"
  }
  